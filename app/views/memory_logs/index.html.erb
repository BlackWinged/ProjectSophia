<p id="notice"><%= notice %></p>

<h1>Memory Logs</h1>

<table>
  <thead>
    <tr>
      <th>Timestamp</th>
      <th>Description</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @memory_logs.each do |memory_log| %>
      <tr>
        <td><%= memory_log.timestamp %></td>
        <td><%= memory_log.description %></td>
        <td><%= memory_log.user %></td>
        <td><%= link_to 'Show', memory_log %></td>
        <td><%= link_to 'Edit', edit_memory_log_path(memory_log) %></td>
        <td><%= link_to 'Destroy', memory_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Memory Log', new_memory_log_path %>
